{
  "name": "fynpo",
  "version": "0.1.2",
  "description": "Supplement tools for using fyn with lerna",
  "main": "index.js",
  "scripts": {},
  "keywords": [
    "fyn",
    "flat",
    "node",
    "modules",
    "node_modules",
    "lerna",
    "repo",
    "mono",
    "monorepo"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/fynpo.git"
  },
  "bin": {
    "fynpo": "cli/fynpo.js"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "item-queue": "^1.0.0",
    "lodash": "^4.17.4",
    "nix-clap": "^1.3.0",
    "visual-logger": "^0.1.3",
    "xsh": "^0.4.2"
  },
  "devDependencies": {
    "electrode-archetype-njs-module-dev": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "reporter": [
