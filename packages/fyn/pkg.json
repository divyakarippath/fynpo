  "description": "Samsclub Membership Application",
  "scripts": {
    "build": "builder run build",
    "dev": "builder run dev",
    "dev-https": "WEBPACK_DEV=true ONEOPS_ENVPROFILE=https-dev builder concurrent server-dev-https server-watch https-proxy",
    "https-proxy": "local-ssl-proxy --source 3443 --target 3000",
    "help": "builder help",
    "hot": "builder run hot",
    "publish-static": "pronto-js -f \"dist/**/{*.js,*.css,*.woff,*.eot,*.ttf,*.jpg,*.jpeg,*.png,*.gif}\" -o \"config/assets.json\"",
    "test": "builder run npm:test && builder run cov-custom",
    "cov-custom": "istanbul check-coverage 'coverage/client/*/coverage.json' --config=.istanbul.yml",
    "start-mock": "node test/automation/mocks/run-mock-server-console.js",
    "test-automation-lint": "eslint --ext .js,.jsx -c test/automation/conf/eslint/.eslintrc-base test/automation/lib test/automation/tests",
    "test-frontend": "builder run test-frontend-cov",
    "test-ui-smoke": "test/automation/tests/lib/script/run-test.sh",
    "test-ui-regression": "test/automation/tests/lib/script/run-test.sh regression",
    "test-ui-smoke-stg": "test/automation/tests/lib/script/run-test-on-stg.sh",
    "test-ui-regression-stg": "test/automation/tests/lib/script/run-test-on-stg.sh regression",
    "test-ui-mock-setup": "test/automation/tests/lib/script/mock-setup.sh",
    "test-setup": "test/automation/tests/lib/script/test-setup.sh",
    "test-ui": "DPRO_LOCATION=./test/automation/conf/data  DPRO=default ./node_modules/.bin/magellan --browser=chrome --config test/automation/magellan.json --serial",
    "server-dev-https": "webpack-dev-server --config ./config/webpack/webpack.config.https-dev.js --progress --colors --port 2992 --https",
    "test-ui-lint": "npm run test-automation-lint"
  },
